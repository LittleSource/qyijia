page {
	background-color: #fafafa;
}

.tui-header-box {
	width: 100%;
	position: fixed;
	left: 0;
	top: 0;
	z-index: 9998;
}

.tui-header {
	width: 100%;
	font-size: 18px;
	line-height: 18px;
	font-weight: 500;
	height: 32px;
	display: flex;
	align-items: center;
	justify-content: center;
}

.tui-icon-box {
	position: relative;
}

.tui-icon-setup {
	margin-left: 16rpx;
}

.tui-mybg-box {
	width: 100%;
	height: 464rpx;
	position: relative;
}

.tui-my-bg {
	width: 100%;
	height: 464rpx;
	display: block;
}

.tui-header-center {
	position: absolute;
	width: 100%;
	height: 128rpx;
	left: 0;
	top: 120rpx;
	padding: 0 30rpx;
	box-sizing: border-box;
	display: flex;
	align-items: center;
}

.tui-avatar {
	flex-shrink: 0;
	width: 128rpx;
	height: 128rpx;
	display: block;
	border-radius: 64rpx;
	overflow: hidden;
}

.tui-info {
	width: 60%;
	padding-left: 30rpx;
}

.tui-nickname {
	font-size: 30rpx;
	font-weight: 500;
	color: #fff;
	display: flex;
	align-items: center;
}

.tui-content-box {
	width: 100%;
	padding: 0 30rpx;
	margin-top: -90rpx;
	box-sizing: border-box;
	position: relative;
	top: -72rpx;
	z-index: 10;
}

.tui-img {
	margin-top: 10rpx;
	width: 100%;
	height: 110rpx;
}

.tui-box {
	width: 100%;
	background: #fff;
	box-shadow: 0 3rpx 20rpx rgba(183, 183, 183, 0.1);
	border-radius: 10rpx;
	overflow: hidden;
}

.tui-order-box {
	height: 208rpx;
}

.tui-cell-header {
	width: 100%;
	height: 74rpx;
	padding: 0 26rpx;
	box-sizing: border-box;
	display: flex;
	align-items: center;
	justify-content: space-between;
}

.tui-cell-title {
	font-size: 30rpx;
	line-height: 30rpx;
	font-weight: 600;
	color: #333;
}

.tui-cell-sub {
	font-size: 26rpx;
	font-weight: 400;
	color: #999;
	padding-right: 28rpx;
}

.tui-order-list {
	width: 100%;
	height: 134rpx;
	padding: 0 30rpx;
	box-sizing: border-box;
	display: flex;
	align-items: center;
	justify-content: space-between;
}

.tui-order-item {
	flex: 1;
	display: flex;
	flex-direction: column;
	align-items: center;
}

.tui-order-text {
	font-size: 26rpx;
	font-weight: 400;
	color: #666;
	padding-top: 4rpx;
}

.tui-order-icon {
	width: 56rpx;
	height: 56rpx;
	display: block;
}

.logo {
	height: 56rpx;
	width: 56rpx;
}

.tui-flex {
	display: flex;
	align-items: center;
}

.tui-cell-name {
	padding-left: 20rpx;
	vertical-align: middle;
	line-height: 30rpx;
}

.tui-list::after {
	left: 94rpx !important
}

.tui-right {
	margin-left: auto;
	margin-right: 34rpx;
	font-size: 26rpx;
	line-height: 1;
	color: #999;
}

.button-none {
	padding: 0;
	background-color: transparent;
	height: 55rpx;
	width: 100% !important;
	border: 0rpx;
	display: inline-flex;
	align-items: center;
	justify-content: left;
	box-sizing: border-box;
	font-size: 30rpx;
	font-weight: normal;
	margin-left: 0rpx !important;
}

.button-none::after {
	display: none;
}