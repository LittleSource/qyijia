page{
    background-color: #fafafa;
}
.container {
    padding-bottom: env(safe-area-inset-bottom);
}

.tui-order-list1 {
    margin-top: 20rpx;
}
.tui-order-list2{
    margin-top: 100rpx;
}

.tui-order-item {
    margin-top: 20rpx;
    border-radius: 10rpx;
    overflow: hidden;
}

.tui-goods-title {
    width: 100%;
    font-size: 28rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.tui-order-status {
    color: #888;
    font-size: 26rpx;
}

.tui-goods-item {
    width: 100%;
    padding: 5rpx 30rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: space-between;
}

.tui-goods-img {
    width: 130rpx;
    height: 130rpx;
    display: block;
    flex-shrink: 0;
}

.tui-goods-center {
    flex: 1;
    padding: 20rpx 8rpx;
    box-sizing: border-box;
}

.tui-goods-name {
    max-width: 310rpx;
    word-break: break-all;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    font-size: 28rpx;
    line-height: 32rpx;
}

.tui-goods-attr {
    font-size: 26rpx;
    color: #555555;
    line-height: 32rpx;
    padding-top: 20rpx;
    word-break: break-all;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.tui-price-right {
    text-align: right;
    color: #888888;
    padding-top: 20rpx;
}
.tui-right-name{
    font-size: 26rpx;
    line-height: 32rpx;
}
.tui-right-sum{
    padding-top: 20rpx;
    font-size: 26rpx;
    line-height: 32rpx;
}
.tui-color-red {
    color: #E41F19;
    padding-right: 30rpx;
}

.tui-goods-price {
    width: 100%;
    display: flex;
    align-items: flex-end;
    justify-content: flex-end;
    font-size: 24rpx;
    line-height: 30rpx;
}

.tui-price-large {
    font-size: 30rpx;
    line-height: 30rpx;
    font-weight: 500;
}
.tui-order-bottom{
    width: 100%;
    height: 80rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    flex-direction: row;
    flex-wrap: nowrap;
    background: #fff;
    padding: 0rpx 30rpx;
    box-sizing: border-box;
}
.tui-order-time{
    font-size: 28rpx;
    color: #555555;
    flex-wrap: nowrap;
}
.tui-order-btn {
    display: flex;
    box-sizing: border-box;
}
.tui-btn-ml {
    margin-left: 5rpx;
}
